{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "FeatureSet",
  "type": "object",
  "required": ["id", "name"],
  "properties": {
    "id": {"type": "string", "format": "uuid"},
    "name": {"type": "string"},
    "description": {"type": ["string", "null"]},
    "created_at": {"type": ["string", "null"], "format": "date-time"}
  },
  "additionalProperties": false
}
